    <fragment
        android:id="@+id/interestingFragment"
        android:name="com.example.myrecipeapp.fragments.InterestingFragment"
        android:label="fragment_interesting"
        tools:layout="@layout/fragment_interesting" />
    <fragment
        android:id="@+id/categoryFragment2"
        android:name="com.example.myrecipeapp.fragments.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.myrecipeapp.fragments.SearchFragment"
        android:label="fragment_search"
